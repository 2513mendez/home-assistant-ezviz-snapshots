{
  "name": "EZVIZ Snapshots API/MQTT (Multi-account)",
  "version": "1.2.3",
  "slug": "ezviz_snapshots_core",
  "description": "Snapshots de EZVIZ por MQTT. Configurable por UI con dos cuentas (campos dedicados) o lista accounts[]. Token por cuenta, cach√© y logs con timestamp.",
  "arch": [
    "armv7",
    "armhf",
    "aarch64",
    "amd64",
    "i386"
  ],
  "startup": "once",
  "boot": "manual",
  "options": {
    "retain": true,
    "quality": 0,
    "debug": false,
    "mqtt_host": "core-mosquitto",
    "mqtt_port": 1883,
    "mqtt_user": "",
    "mqtt_password": "",
    "account1_id": "",
    "account1_app_key": "",
    "account1_app_secret": "",
    "account2_id": "",
    "account2_app_key": "",
    "account2_app_secret": "",
    "accounts": [],
    "camaras": []
  },
  "schema": {
    "retain": "bool",
    "quality": "int?",
    "debug": "bool?",
    "mqtt_host": "str",
    "mqtt_port": "int",
    "mqtt_user": "str",
    "mqtt_password": "str",
    "account1_id": "str?",
    "account1_app_key": "str?",
    "account1_app_secret": "str?",
    "account2_id": "str?",
    "account2_app_key": "str?",
    "account2_app_secret": "str?",
    "accounts": [
      {
        "id": "str",
        "app_key": "str",
        "app_secret": "str",
        "quality": "int?"
      }
    ],
    "camaras": [
      {
        "nombre": "str",
        "serial": "str",
        "channel": "int",
        "quality": "int?",
        "account": "str"
      }
    ]
  },
  "init": false,
  "host_network": false,
  "map": [
    "config:rw"
  ],
  "privileged": [],
  "apparmor": false
}
